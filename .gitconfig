	ui = auto
[branch "master"]
	merge = refs/heads/master
[core]
	excludesfile = ~/.gitignore
	# autocrlf = input
	# quotepath = false
	# safecrlf = warn
     attributesfile = ~/.gitattributes
[merge]
	tool = emerge
[rerere]
	enabled = 1
[i18n]
    logoutputencoding = UTF-8
[diff "elisp"]
    xfuncname="^(\\((def).*)$"
[color]
    diff = auto
    status = auto
    branch = auto
[format]
    pretty = "Commit:  %C(cyan)%H%nAuthor:  %C(green)%aN <%aE>%nDate:    (%C(yellow)%ar%Creset) %ai%n%n     %C(white)%s%n%b"
